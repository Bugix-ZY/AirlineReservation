<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_employee_delete_reg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="io.github.bugix_zy.airlinereservation.EmployeeDeleteRegActivity">

    <TextView
        android:text="Flight No."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="152dp"
        android:id="@+id/textView13"
        android:textSize="30sp"
        android:layout_alignTop="@+id/deleteRegNoEdit"
        android:layout_toStartOf="@+id/deleteRegNoEdit" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_marginEnd="320dp"
        android:layout_marginTop="117dp"
        android:id="@+id/deleteRegNoEdit"
        android:textSize="30sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="BACK"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/deleteRegNoEdit"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="156dp"
        android:id="@+id/deleteRegBack"
        android:textSize="30sp"
        android:onClick="onClickedDeleteRegCancel"
        android:layout_width="130dp" />

    <Button
        android:text="Delete"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_alignEnd="@+id/deleteRegNoEdit"
        android:layout_marginEnd="71dp"
        android:id="@+id/deleteRegConfirm"
        android:onClick="onClickedDeleteRegButton"
        android:textSize="30sp"
        android:layout_alignTop="@+id/deleteRegBack"
        android:layout_alignBottom="@+id/deleteRegBack" />
</RelativeLayout>
